import numpy as np
import mat.plotlib.pyplot as plt
%matplotlib inline
score=[]
keys=[]
for i in Dict.values():
  score.append(i)
for i in range(1,31):
  keys.append(i)
Days=np.array(keys)
score=np.array(score)
fig,ax1=plt.subplots()
color='tab:red'
ax1.set_xlabel('score')
ax1.set_ylabel('Days',color=color)
ax1.plot(score,Days,color=color)
plt.show()
Mean=np.mean(score)
Median=np.median(score)
Min=np.min(score)
Max=np.max(score)
